        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NoReminder">
          <DataField>NoReminder</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Email">
          <DataField>Email</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Mobile">
          <DataField>Mobile</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>-- step 1 -- get all the sales info
drop table uTEMP_CCS1
;
select staff.name As staffid, staff.firstname, staff.lastname, salelines.regno, salelines.checksheetno, salelines.qty
  into uTEMP_CCS1
from sales, salelines, staff
where sales.sid = salelines.sid
  and sales.teller = staff.tid
  and sales.phase = 1
  and (sales.completeddt between @StartDate and (@EndDate + 1))
  and sales.sid not in (select sid from payments where type = 24)
  and salelines.pid in (select pid from products where plucode in (@Product))
  and sales.phys_location in (select location_id from locations where name in (@Location))
order by staff.name

-- step 2 -- get all the cust capture info
drop table uTEMP_CCS2
;
select vtnz_custcapture.entrydt, vtnz_custcapture.regno, vtnz_custcapture.checksheet, vtnz_custcapture.noreminder,
vtnz_custcapture.email, vtnz_custcapture.phone2
  into uTEMP_CCS2
from vtnz_custcapture, uTEMP_CCS1
where (vtnz_custcapture.regno = uTEMP_CCS1.regno
  and vtnz_custcapture.checksheet = uTEMP_CCS1.checksheetno)

-- step 3 -- get all the original (initial) cust capture record created at time of sale
drop table uTEMP_CCS3
;
select min(entrydt) As entrydt, regno, checksheet
  into uTEMP_CCS3
from uTEMP_CCS2
group by regno, checksheet
order by regno

drop table uTEMP_CCS
;
create table uTEMP_CCS 
(staffid char(10),
 firstname char(32),
 lastname char(32),
 sales integer,
 noreminder integer,
 email integer,
 mobile integer)

-- step 4 -- count how many SALES made
insert into uTEMP_CCS
select staffid, firstname, lastname, sum(qty), 0, 0, 0
from uTEMP_CCS1
group by staffid, firstname, lastname
order by staffid, firstname, lastname

-- step 5 -- count how many NO REMINDERs are flagged YES
insert into uTEMP_CCS
select uTEMP_CCS1.staffid, uTEMP_CCS1.firstname, uTEMP_CCS1.lastname, 0, count(uTEMP_CCS2.noreminder), 0, 0
from uTEMP_CCS1, uTEMP_CCS2, uTEMP_CCS3
where uTEMP_CCS1.regno = uTEMP_CCS2.regno
  and uTEMP_CCS1.checksheetno = uTEMP_CCS2.checksheet
  and uTEMP_CCS2.regno = uTEMP_CCS3.regno
  and uTEMP_CCS2.checksheet = uTEMP_CCS3.checksheet
  and uTEMP_CCS2.entrydt = uTEMP_CCS3.entrydt
  and uTEMP_CCS2.noreminder = 1
group by uTEMP_CCS1.staffid, uTEMP_CCS1.firstname, uTEMP_CCS1.lastname
order by uTEMP_CCS1.staffid

-- step 6 -- count how many EMAILs completed
insert into uTEMP_CCS
select uTEMP_CCS1.staffid, uTEMP_CCS1.firstname, uTEMP_CCS1.lastname, 0, 0, count(uTEMP_CCS2.email), 0
from uTEMP_CCS1, uTEMP_CCS2, uTEMP_CCS3
where uTEMP_CCS1.regno = uTEMP_CCS2.regno
  and uTEMP_CCS1.checksheetno = uTEMP_CCS2.checksheet
  and uTEMP_CCS2.regno = uTEMP_CCS3.regno
  and uTEMP_CCS2.checksheet = uTEMP_CCS3.checksheet
  and uTEMP_CCS2.entrydt = uTEMP_CCS3.entrydt
  and uTEMP_CCS2.email is not null
group by uTEMP_CCS1.staffid, uTEMP_CCS1.firstname, uTEMP_CCS1.lastname
order by uTEMP_CCS1.staffid

-- step 7 -- count how many MOBILE NOs completed
insert into uTEMP_CCS
select uTEMP_CCS1.staffid, uTEMP_CCS1.firstname, uTEMP_CCS1.lastname, 0, 0, 0, count(uTEMP_CCS2.phone2)
from uTEMP_CCS1, uTEMP_CCS2, uTEMP_CCS3
where uTEMP_CCS1.regno = uTEMP_CCS2.regno
  and uTEMP_CCS1.checksheetno = uTEMP_CCS2.checksheet
  and uTEMP_CCS2.regno = uTEMP_CCS3.regno
  and uTEMP_CCS2.checksheet = uTEMP_CCS3.checksheet
  and uTEMP_CCS2.entrydt = uTEMP_CCS3.entrydt
  and uTEMP_CCS2.phone2 is not null
group by uTEMP_CCS1.staffid, uTEMP_CCS1.firstname, uTEMP_CCS1.lastname
order by uTEMP_CCS1.staffid

-- step 8 -- final select for output
select staffid As StaffID, firstname As Firstname, lastname As Lastname, sum(sales) As Sales, 
sum(noreminder) As NoReminder, sum(email) As Email, sum(mobile) As Mobile
from uTEMP_CCS
group by staffid, firstname, lastname
order by staffid</CommandText>
