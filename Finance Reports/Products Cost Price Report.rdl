        <Field Name="SupplySource">
          <DataField>SupplySource</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>select r_departments.description As ProductGroup, products.plucode As ProductID, products.description As ProductDesc, 
products.costprice As CostPrice, products.supplysource As SupplySource
from products, r_departments
where products.r_depid = r_departments.depid
and supplysource in (@SupplySource)
order by r_departments.depid, products.plucode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@SupplySource">
            <Value>=Parameters!SupplySource.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
