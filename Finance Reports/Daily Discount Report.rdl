  <DataSets>
    <DataSet Name="Main">
      <Fields>
        <Field Name="disc_name">
          <DataField>disc_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="location_id">
          <DataField>location_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="disc_1_p">
          <DataField>disc_1_p</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="discid_1">
          <DataField>discid_1</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     Discounts.disc_name, Locations.name, Locations.location_id, Salelines.disc_1_p, Salelines.discid_1
FROM         Sales INNER JOIN
                      Salelines ON Salelines.Sid = Sales.Sid INNER JOIN
                      Products ON Products.pid = Salelines.Pid INNER JOIN
                      Locations ON Locations.location_id = Sales.phys_location LEFT OUTER JOIN
                      Discounts ON Discounts.disc_id = Salelines.discid_1
WHERE     (Sales.CompletedDT BETWEEN @StartDate AND @EndDate + 1) AND (Locations.location_id IN (@Location)) AND (Salelines.discid_1 &lt;&gt; 0) AND 
                      (Discounts.disc_id &lt;&gt; 4) AND (Discounts.disc_id &lt;&gt; 22) AND (Sales.Phase = 1)
ORDER BY Discounts.disc_name, Locations.location_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Locatons">
      <Fields>
        <Field Name="location_id">
          <DataField>location_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="idName">
          <DataField>idName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     location_id, name, CAST(location_id AS varchar(50)) + ' - ' + name AS idName
FROM         Locations
WHERE     (location_id &lt; 600 AND location_id &lt;&gt; 536)
ORDER BY location_id</CommandText>
      </Query>
    </DataSet>
  </DataSets>
