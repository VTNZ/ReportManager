        </Field>
        <Field Name="ReportGroup">
          <DataField>ReportGroup</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DiscountGiven">
          <DataField>DiscountGiven</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CardType">
          <DataField>CardType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Registration">
          <DataField>Registration</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     StationName, PluCode, ProductID, CompletedDate, Quantity, SaleID, ProductDesc, CustomerID, CustomerName, ReportGroup, TotalPrice, UnitPrice, 
                      DiscountGiven, CardType, Registration
FROM         (SELECT     name AS StationName, plucode AS PluCode, Pid AS ProductID, CompletedDT AS CompletedDate, qty AS Quantity, Sid AS SaleID, 
                                              description AS ProductDesc, Cid AS CustomerID, Expr1 AS CustomerName, r_depid AS ReportGroup, totalprice AS TotalPrice, 
                                              Unitprice AS UnitPrice, RegNo AS Registration, CASE WHEN (totalprice IS NULL OR
                                              unitprice IS NULL OR
                                              totalprice = 0 OR
                                              unitprice = 0) THEN 0 ELSE 100 - ((abs(totalprice) / abs(unitprice)) * 100) END AS DiscountGiven, CardType
                       FROM          (SELECT DISTINCT 
                                                                      dbo.Sales.CompletedDT, dbo.Products.plucode, dbo.Salelines.qty, dbo.Salelines.totalprice, dbo.Locations.name, 
                                                                      dbo.Salelines.Pid, dbo.Salelines.Sid, dbo.Products.description, dbo.Customers.Cid, dbo.Customers.Name AS Expr1, 
                                                                      dbo.Products.r_depid, dbo.Salelines.CheckSheetNo, dbo.Salelines.regno AS RegNo, dbo.Products.unitprice AS Unitprice, 
                                                                      dbo.Products.costprice AS CostPrice, dbo.eftposreceipts.cardtype AS CardType
                                               FROM          dbo.Locations INNER JOIN
                                                                      dbo.Sales ON dbo.Locations.location_id = dbo.Sales.location INNER JOIN
                                                                      dbo.Products INNER JOIN
                                                                      dbo.Salelines ON dbo.Products.pid = dbo.Salelines.Pid ON dbo.Sales.Sid = dbo.Salelines.Sid INNER JOIN
                                                                      dbo.Customers ON dbo.Sales.Cid = dbo.Customers.Cid LEFT OUTER JOIN
                                                                      dbo.eftposreceipts ON dbo.Sales.Sid = dbo.eftposreceipts.sid
                                               WHERE      (dbo.Sales.CompletedDT BETWEEN @StartDate AND @EndDate + 1) AND (dbo.Sales.Cid != 303994) AND (dbo.Salelines.Pid IN
                                                                          (SELECT     pid
                                                                            FROM          dbo.Products AS Products_1
                                                                            WHERE      (r_depid IN (@ProductGroup)))) AND (dbo.Products.r_depid IN (@ProductGroup)) AND 
                                                                      (dbo.eftposreceipts.cardtype IN (@CardType))) AS a) AS b
ORDER BY CustomerName, PluCode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProductGroup">
            <Value>=Parameters!ProductGroup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CardType">
            <Value>=Parameters!CardType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ProductGroup">
      <Fields>
        <Field Name="depid">
          <DataField>depid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     depid, RTRIM(description) AS description
FROM         r_departments
ORDER BY description</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="Products">
      <Fields>
        <Field Name="pid">
          <DataField>pid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="plucode">
          <DataField>plucode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="r_depid">
          <DataField>r_depid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="pludesc">
          <DataField>pludesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     pid, description, plucode, r_depid, CAST(plucode AS VarChar) + ' -  ' + description AS pludesc
FROM         Products
WHERE     (r_depid IN (@ProductGroup))
ORDER BY plucode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProductGroup">
            <Value>=Parameters!ProductGroup.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
