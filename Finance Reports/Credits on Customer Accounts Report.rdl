      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>SELECT     Branch.description AS BranchDesc, Locations.location_id, Locations.name, Sales.saletotal, 
                      CASE WHEN Sales.SaleTotal &lt;= 0 THEN COUNT(Sales.SaleTotal) ELSE 0 END AS Credits, 
                      CASE WHEN Sales.SaleTotal &gt; 0 THEN COUNT(Sales.SaleTotal) ELSE 0 END AS TotalSales, CAST(CONVERT(char(11), Sales.CompletedDT, 113) 
                      AS datetime) AS DateOnly
FROM         Branch INNER JOIN
                      Locations ON Branch.branch = Locations.branch INNER JOIN
                      Sales INNER JOIN
                      Customers ON Sales.Cid = Customers.Cid ON Locations.location_id = Sales.phys_location
WHERE     (Sales.CompletedDT BETWEEN @StartDate AND @EndDate + 1) AND (Customers.PrePay = 0) AND (Sales.Phase = 1)
GROUP BY Branch.description, Locations.location_id, Locations.name, Sales.saletotal, CAST(CONVERT(char(11), Sales.CompletedDT, 113) AS datetime)
ORDER BY BranchDesc, Locations.location_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
