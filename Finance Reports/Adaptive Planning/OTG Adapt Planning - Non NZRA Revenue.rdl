      <Query>
        <DataSourceName>OTGDatasource</DataSourceName>
        <CommandText>drop table uTEMP_FMISRevenueNonOTG
;
create table uTEMP_FMISRevenueNonOTG
(location int,
 description varchar(32),
 dailysales datetime,
 plucode varchar(16),
 totalprice money,
 volume int)
;
insert into uTEMP_FMISRevenueNonOTG (location, description, dailysales, plucode, totalprice, volume)
select sales.location [Plan], r_departments.description [Product],
       dateadd(dd,0,datediff(dd,0,completeddt)) as DailySales,
       products.plucode [ProductPosCode], sum(totalprice) [TotalPrice], sum(qty) [Volume]
  from sales, salelines, products, r_departments, locations
 where sales.sid = salelines.sid
   and salelines.pid = products.pid
   and products.r_depid = r_departments.depid
   and sales.location = locations.location_id
   and sales.phase = 1
   and completeddt between @StartDate and @EndDate
   and (salelines.pid in (select pid from uFMISProducts where NonNZTA = 'Y')
     or products.r_depid = 3)
   and locations.parent = 0
-- hardcoding 127 as temp solution --
   and locations.location_id &lt;&gt; 127
-- hardcoding 127 as temp solution --
group by dateadd(dd,0, datediff(dd,0,completeddt)), products.plucode, r_departments.description, sales.location
;
insert into uTEMP_FMISRevenueNonOTG (location, description, dailysales, plucode, totalprice, volume)
select locations.parent [Plan], r_departments.description [Product],
       dateadd(dd,0,datediff(dd,0,completeddt)) as DailySales,
       products.plucode [ProductPosCode], sum(totalprice) [TotalPrice], sum(qty) [Volume]
  from sales, salelines, products, r_departments, locations
 where sales.sid = salelines.sid
   and salelines.pid = products.pid
   and products.r_depid = r_departments.depid
   and sales.location = locations.location_id
   and sales.phase = 1
   and completeddt between @StartDate and @EndDate
   and (salelines.pid in (select pid from uFMISProducts where NonNZTA = 'Y')
     or products.r_depid = 3)
   and locations.parent &lt;&gt; 0
-- hardcoding 127 as temp solution --
   and locations.location_id &lt;&gt; 127
-- hardcoding 127 as temp solution --
group by dateadd(dd,0, datediff(dd,0,completeddt)), products.plucode, r_departments.description, locations.parent
;
select location [Plan], description [Product], '' [Account],
       dateadd(dd,0,datediff(dd,0,dailysales)) as DailySales,
       plucode [ProductPOSCode], sum(totalprice) [TotalPrice], sum(volume) [Volume]
  from uTEMP_FMISRevenueNonOTG
group by dateadd(dd,0, datediff(dd,0,dailysales)), plucode, description, location
order by dateadd(dd,0, datediff(dd,0,dailysales)), location, description, plucode</CommandText>
