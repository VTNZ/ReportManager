      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>SELECT     Payments.type, Payments.amount, Payments.relatedtype, Sales.Sid, Sales.CompletedDT, Sales.phys_location, Locations.name, 
                      RTRIM(r_PaymentTypes.TypeStr) AS TypeStr, CASE WHEN Payments.type = 1 THEN SUM(Payments.amount) END AS 'Cash', 
                      CASE WHEN Payments.type = 6 THEN SUM(Payments.amount) END AS 'Change', CASE WHEN Payments.type = 2 THEN SUM(Payments.amount) 
                      END AS 'Cheque', CASE WHEN Payments.type = 3 THEN SUM(Payments.amount) END AS 'EFTPOS', 
                      CASE WHEN Payments.type = 8 THEN CASE WHEN Payments.relatedtype = 1 THEN SUM(Payments.amount) END END AS 'Return1', 
                      CASE WHEN Payments.type = 8 THEN CASE WHEN Payments.relatedtype = 2 THEN SUM(Payments.amount) END END AS 'Return2', 
                      CASE WHEN Payments.type = 8 THEN CASE WHEN Payments.relatedtype = 3 THEN SUM(Payments.amount) END END AS 'Return3', 
                      Locations.location_id
FROM         Payments INNER JOIN
                      Sales ON Payments.Sid = Sales.Sid INNER JOIN
                      r_PaymentTypes ON Payments.type = r_PaymentTypes.Code INNER JOIN
                      Locations ON Sales.phys_location = Locations.location_id
WHERE     (Payments.type IN (1, 2, 3, 6, 8)) AND (Sales.CompletedDT BETWEEN CONVERT(varchar, GETDATE() - 1, 102) + ' 12:00:00 pm' AND CONVERT(varchar, 
                      GETDATE(), 102) + ' 12:00:00 pm') AND (Sales.Phase = 1)
GROUP BY Payments.type, Payments.amount, Payments.relatedtype, Sales.CompletedDT, Sales.phys_location, Locations.name, r_PaymentTypes.TypeStr, 
                      Sales.Sid, Locations.location_id
ORDER BY Locations.name, TypeStr</CommandText>
      </Query>
    </DataSet>
