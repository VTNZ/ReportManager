    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Location">
          <DataField>Location</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PluCode">
          <DataField>PluCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateCompleted">
          <DataField>DateCompleted</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="NZTAPaymentNo">
          <DataField>NZTAPaymentNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustomerID">
          <DataField>CustomerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalPrice">
          <DataField>TotalPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SaleID">
          <DataField>SaleID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>select		'Location' =
			case locations.parent
			when 0 then locations.location_id
			else locations.parent
			end,	-- If this is an out station classify it's location as it's parent location
			sales.CompletedDT as 'DateCompleted',
			products.plucode as 'PluCode',
			LTSAPmtNo as 'NZTAPaymentNo',
			sales.cid as 'CustomerID',
			sales.[sid] as 'SaleID',
			salelines.totalprice as 'TotalPrice'
from		salelines
left join	sales
on			salelines.[sid] = sales.[sid]
left join	products
on			salelines.pid = products.pid
left join	locations
on			sales.location = Locations.location_id
where		CompletedDT between @StartDate and @EndDate and products.plucode in(@PluCode)
order by	sales.location, sales.CompletedDT, products.plucode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PluCode">
            <Value>=Parameters!PluCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
