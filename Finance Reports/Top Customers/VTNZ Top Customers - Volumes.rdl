        <Field Name="CustomerName">
          <DataField>CustomerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductGroup">
          <DataField>ProductGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Volume">
          <DataField>Volume</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>select sales.location [Plan], sales.cid [CustomerNo], customers.name [CustomerName],
       r_departments.description [ProductGroup], sum(qty) [Volume],
       Month(completeddt) as Month
  from sales, salelines, products, r_departments, locations, customers
 where sales.sid = salelines.sid
   and salelines.pid = products.pid
   and products.r_depid = r_departments.depid
   and sales.location = locations.location_id
   and sales.cid = customers.cid
   and sales.phase = 1
   and completeddt between @StartDate and @EndDate
   and products.r_depid in (select prodgroup from uFMISTopCustomers where sales = 'Y')
   and locations.location_id &lt;&gt; 127
   and sales.cid not in (999999,888888)
group by sales.location, sales.cid, customers.name, r_departments.description, Month(completeddt)
order by sales.cid, sales.location, r_departments.description</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
