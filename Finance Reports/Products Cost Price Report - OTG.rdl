        <Field Name="CostPrice">
          <DataField>CostPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>OTGDatasource</DataSourceName>
        <CommandText>select r_departments.description As ProductGroup, products.plucode As ProductID, products.description As ProductDesc, products.costprice As CostPrice
from products, r_departments
where products.r_depid = r_departments.depid
order by r_departments.depid, products.plucode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
