  <DataSets>
    <DataSet Name="Main">
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="location_id">
          <DataField>location_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="totalprice">
          <DataField>totalprice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="pid">
          <DataField>pid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="qty">
          <DataField>qty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     Locations.name, Locations.location_id, Salelines.totalprice, Products.pid, Branch.description, Salelines.qty
FROM         Sales INNER JOIN
                      Salelines ON Salelines.Sid = Sales.Sid INNER JOIN
                      Products ON Products.pid = Salelines.Pid INNER JOIN
                      Locations ON Locations.location_id = Sales.phys_location INNER JOIN
                      Branch ON Locations.branch = Branch.branch
WHERE     (Sales.CompletedDT BETWEEN @StartDate AND @EndDate + 1) AND (Locations.location_id IN (@Location)) AND (Products.pid IN (@Products)) AND 
                      (Branch.description IN (@Area)) AND (Sales.Phase = 1)
ORDER BY Locations.location_id, Locations.name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Products">
            <Value>=Parameters!Products.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Area">
            <Value>=Parameters!Area.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Locations">
      <Fields>
        <Field Name="location_id">
          <DataField>location_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="idName">
          <DataField>idName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     Locations.location_id, Locations.name, CAST(Locations.location_id AS varchar(50)) + ' - ' + Locations.name AS idName, Branch.description
FROM         Locations INNER JOIN
                      Branch ON Locations.branch = Branch.branch
WHERE     (Locations.location_id &lt; 600) AND (Branch.description IN (@Area))
ORDER BY Locations.location_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Area">
            <Value>=Parameters!Area.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Products">
      <Fields>
        <Field Name="pid">
          <DataField>pid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="plucode">
          <DataField>plucode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="idDescription">
          <DataField>idDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="depid">
          <DataField>depid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="description_1">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     Products.pid, Products.description, Products.plucode, Products.plucode + ' - ' + Products.description AS idDescription, r_departments.depid, 
                      r_departments.description
FROM         Products INNER JOIN
                      r_departments ON Products.r_depid = r_departments.depid
WHERE     (r_departments.description IN (@ProductGroup))
ORDER BY Products.plucode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProductGroup">
            <Value>=Parameters!ProductGroup.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="ProductGroup">
      <Fields>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="depid">
          <DataField>depid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     RTRIM(description) AS description, depid
FROM         r_departments
ORDER BY description</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="Area">
      <Fields>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     RTRIM(description) AS description
FROM         Branch
ORDER BY description</CommandText>
      </Query>
    </DataSet>
  </DataSets>
