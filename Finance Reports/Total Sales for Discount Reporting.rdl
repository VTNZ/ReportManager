  <DataSources>
    <DataSource Name="VTNZSharedDatasource">
      <DataSourceReference>VTNZSharedDataSource</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>f476de0a-ae79-4199-8320-24d1fc4972a7</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     Locations.location_id, locations.name, Salelines.totalprice, Sales.CompletedDT
FROM         Sales INNER JOIN
                      Salelines ON Salelines.Sid = Sales.Sid INNER JOIN
                      Locations ON Locations.location_id = Sales.phys_location
WHERE phase = 1 AND (Sales.CompletedDT BETWEEN @StartDate AND @EndDate + 1) AND (Salelines.pid NOT IN (select pid from uFMISProducts where Business_Development_Dashboard_Exclusions = 'Y') AND Salelines.pid &lt;&gt; 69 AND Locations.Location_id &lt;&gt; 536)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="location_id">
          <DataField>location_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="totalprice">
          <DataField>totalprice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CompletedDT">
          <DataField>CompletedDT</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
