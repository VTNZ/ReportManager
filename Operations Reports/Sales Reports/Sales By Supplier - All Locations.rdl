        </Field>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Volume">
          <DataField>Volume</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Cost">
          <DataField>Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>SELECT suppliers.spid SupplierID, suppliers.name Supplier, 
suppliers.name + ' - ' + CAST(suppliers.spid AS varchar(50)) AS idSupplier,
products.plucode ProductCode, products.description ProductName, sum(salelines.qty) Volume, sum(salelines.totalprice) Amount, sum(products.costprice) Cost
FROM suppliers, products, sales, salelines
WHERE sales.sid = salelines.sid
AND salelines.pid = products.pid
AND products.r_spid = suppliers.spid
AND completeddt BETWEEN @StartDate and @EndDate
AND (suppliers.spid IN (@Supplier))
AND sales.phase = 1
GROUP BY suppliers.spid, suppliers.name, products.plucode, products.description
ORDER BY sum(salelines.qty) desc, products.description</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Supplier">
            <Value>=Parameters!Supplier.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet3">
      <Fields>
