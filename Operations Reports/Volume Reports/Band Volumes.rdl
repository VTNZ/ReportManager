      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>SELECT     PriceBandMap.PriceBand, r_departments.depid, Locations.location_id, Sales.CompletedDT, Salelines.qty, Salelines.totalprice, 
                      r_departments.description, Locations.name, Locations.parent
FROM         Locations INNER JOIN
                      PriceBandMap ON Locations.location_id = PriceBandMap.LocationStart INNER JOIN
                      Salelines INNER JOIN
                      Products ON Salelines.Pid = Products.pid INNER JOIN
                      Sales ON Salelines.Sid = Sales.Sid INNER JOIN
                      r_departments ON Products.r_depid = r_departments.depid ON Locations.location_id = Sales.location AND 
                      PriceBandMap.DeptStart = r_departments.depid
WHERE     (PriceBandMap.PriceBand IN (@PriceBand)) AND (r_departments.depid IN (@Dept)) AND (Locations.location_id IN (@Station)) AND 
                      (Sales.CompletedDT BETWEEN @StartDate AND @EndDate + 1)
ORDER BY Locations.location_id, Sales.CompletedDT</CommandText>
        <QueryParameters>
          <QueryParameter Name="@PriceBand">
            <Value>=Parameters!PriceBand.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Station">
            <Value>=Parameters!Station.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="PriceBands">
      <Fields>
        <Field Name="PriceBand">
          <DataField>PriceBand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DeptStart">
          <DataField>DeptStart</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>SELECT DISTINCT PriceBand, DeptStart
FROM         PriceBandMap
WHERE     (DeptStart IN (@dept))
ORDER BY PriceBand</CommandText>
        <QueryParameters>
          <QueryParameter Name="@dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
