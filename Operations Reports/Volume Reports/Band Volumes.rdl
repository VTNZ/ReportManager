          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>SELECT     depid, description
FROM         r_departments
ORDER BY description</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="Station">
      <Fields>
        <Field Name="location_id">
          <DataField>location_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="idLocation">
          <DataField>idLocation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="depid">
          <DataField>depid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>SELECT DISTINCT 
                      Locations.location_id, Locations.name, CAST(Locations.location_id AS Varchar(50)) + ' - ' + Locations.name AS idLocation, r_departments.depid
FROM         Locations INNER JOIN
                      PriceBandMap ON Locations.location_id = PriceBandMap.LocationStart INNER JOIN
                      r_departments ON PriceBandMap.DeptStart = r_departments.depid
WHERE     (PriceBandMap.PriceBand IN (@PriceBand)) AND (r_departments.depid IN (@Dept))
ORDER BY Locations.location_id, Locations.name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@PriceBand">
            <Value>=Parameters!PriceBand.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
