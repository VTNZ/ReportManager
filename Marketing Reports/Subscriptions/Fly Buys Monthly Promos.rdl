    <DataSet Name="Main">
      <Fields>
        <Field Name="TheMonth">
          <DataField>TheMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TheYear">
          <DataField>TheYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BonusPoints">
          <DataField>BonusPoints</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CouponCode">
          <DataField>CouponCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalPoints">
          <DataField>TotalPoints</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>declare @FirstDayOfLastMonth datetime, @FirstDayOfMonth datetime
set @FirstDayOfMonth = convert(datetime, convert(char(7), getdate(), 120) + '-01') 
set @FirstDayOfLastMonth = dateadd(mm, -1, convert(datetime, convert(char(7), getdate(), 120) + '-01')) 

SELECT     Month(@FirstDayOfLastMonth) As TheMonth, Year(@FirstDayOfLastMonth) As TheYear, dbo.temp_FlyBuys.CouponCode, CASE WHEN promotions.u_Calc = 'Y' THEN CASE WHEN temp_flybuys.TotalPoints &lt; 0 THEN 0 - ((temp_flybuys.BonusPoints * temp_flybuys.TotalPoints)
                       - temp_flybuys.TotalPoints) ELSE (temp_flybuys.BonusPoints * temp_flybuys.TotalPoints) 
                      - temp_flybuys.TotalPoints END ELSE temp_flybuys.BonusPoints END AS BonusPoints, temp_flybuys.TotalPoints
FROM         dbo.temp_FlyBuys
INNER JOIN Promotions ON temp_flybuys.CouponCode = Promotions.description
WHERE     (dbo.temp_FlyBuys.CompletedDT  &gt;= @FirstDayOfLastMonth and dbo.temp_FlyBuys.CompletedDT &lt; @FirstDayOfMonth )
		AND dbo.temp_FlyBuys.CouponCode &lt;&gt; 'NULL' AND dbo.temp_FlyBuys.ParentDetail = 'P'
ORDER BY dbo.temp_FlyBuys.CouponCode</CommandText>
      </Query>
    </DataSet>
  </DataSets>
