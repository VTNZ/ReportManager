      <Query>
        <DataSourceName>VTNZSharedDataSource</DataSourceName>
        <CommandText>SELECT Products.pid, Products.description, Products.shortname, Products.plucode, 
Products.r_depid, Products.sellflags, r_departments.description AS ProductGroup, Products.glacct,
barcodes.Barcode As Barcode
FROM Products INNER JOIN r_departments ON Products.r_depid = r_departments.depid
left outer join barcodes on barcodes.pid = Products.pid
ORDER BY Products.pid</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
