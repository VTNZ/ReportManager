  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="LabourID">
          <DataField>LabourID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="cnt">
          <DataField>cnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LabourErrors">
          <DataField>LabourErrors</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DOR</DataSourceName>
        <CommandText>select		LabourID, COUNT(1) as cnt,
		case when COUNT(1) = 38 then 'ok' else 'err' end as LabourErrors
from		LabourHours
where		LabourID in (select LabourID from Labour where LabourDate between @LabourDate and @LabourDate+31)
group by	LabourID
having		not COUNT(1) = 38</CommandText>
        <QueryParameters>
          <QueryParameter Name="@LabourDate">
            <Value>=Parameters!LabourDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
