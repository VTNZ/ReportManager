  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="LocationNo">
          <DataField>LocationNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AreaID">
          <DataField>AreaID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LabourPending">
          <DataField>LabourPending</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AreaName">
          <DataField>AreaName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LabourFinalised">
          <DataField>LabourFinalised</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LocationName">
          <DataField>LocationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DOR</DataSourceName>
        <CommandText>select			--loc.AreaID, loc.AreaName,
				a.location_id AreaID, a.[name] as AreaName,
				loc.LocationNo, loc.LocationName,
				case when labpend.LabourPending is not null then labpend.LabourPending else 0 end as LabourPending,
				case when labfinal.LabourFinalised is not null then labfinal.LabourFinalised else 0 end as LabourFinalised
from			(select * from GetLocationsView where ParentNo = 0 and LocationNo not in(1, 99, 201)) as loc
left outer join
(
	select			lab.LocationNo, sum(1) as LabourPending
	from			Labour as lab
	where			lab.LabourDate between @StartDate and @EndDate and lab.[Status] = 'Pending'
	group by		lab.LocationNo
) labpend
on				loc.LocationNo = labpend.LocationNo
left outer join
(
	select			lab.LocationNo, sum(1) as LabourFinalised
	from			Labour as lab
	where			lab.LabourDate between @StartDate and @EndDate and lab.[Status] = 'Finalised'
	group by		lab.LocationNo
) labfinal
on				loc.LocationNo = labfinal.LocationNo
left join		DORposlive.[pos_live].dbo.uAreas a
on				loc.AreaID = a.location_id
order by		loc.AreaID, loc.LocationNo</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
