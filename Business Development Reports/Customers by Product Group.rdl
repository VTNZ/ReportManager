        </Field>
        <Field Name="r_depid">
          <DataField>r_depid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="qty">
          <DataField>qty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="totalprice">
          <DataField>totalprice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT DISTINCT 
                      Customers.Cid, Customers.Name, Customers.Contact, Customers.Address1, Customers.Address2, Customers.City, Customers.Postcode, 
                      Products.r_depid, r_departments.description, Salelines.qty, Salelines.totalprice
FROM         Customers INNER JOIN
                      Sales ON Customers.Cid = Sales.Cid INNER JOIN
                      Salelines ON Sales.Sid = Salelines.Sid INNER JOIN
                      Products ON Salelines.Pid = Products.pid INNER JOIN
                      r_departments ON Products.r_depid = r_departments.depid
WHERE     (Products.r_depid IN (@ProductGroup)) AND (Customers.Cid &lt;&gt; 999999) AND (Customers.Cid &lt;&gt; 888888) AND (Customers.Cid &lt;&gt; 555555) AND 
                      (Sales.Phase = 1)
ORDER BY Customers.Cid, r_departments.description</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProductGroup">
            <Value>=Parameters!ProductGroup.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="ProductGroups">
      <Fields>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="depid">
          <DataField>depid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>VTNZSharedDatasource</DataSourceName>
        <CommandText>SELECT     RTRIM(description) AS description, depid
FROM         r_departments
ORDER BY description</CommandText>
      </Query>
    </DataSet>
  </DataSets>
