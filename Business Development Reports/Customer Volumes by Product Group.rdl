        </Field>
        <Field Name="ProductDesc">
          <DataField>ProductDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="custidnull">
          <DataField>custidnull</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PosLive</DataSourceName>
        <CommandText>SELECT     Customers.Cid, Sales.CompletedDT, Salelines.qty, Salelines.totalprice, Customers.Name, r_departments.depid, r_departments.description, Sales.Sid, 
                      Products.plucode, Products.description AS ProductDesc, @CustID AS custidnull
FROM         Salelines INNER JOIN
                      Sales ON Salelines.Sid = Sales.Sid INNER JOIN
                      Products ON Salelines.Pid = Products.pid INNER JOIN
                      r_departments ON Products.r_depid = r_departments.depid INNER JOIN
                      Customers ON Sales.Cid = Customers.Cid
WHERE     (Sales.CompletedDT BETWEEN @StartDate AND @EndDate + 1) AND (Customers.Name LIKE '%' + @CustID + '%') AND 
                      (r_departments.depid IN (@Dept)) OR
                      (Sales.CompletedDT BETWEEN @StartDate AND @EndDate + 1) AND (r_departments.depid IN (@Dept)) AND (@CustID IS NULL) AND (Sales.Phase = 1)
ORDER BY Salelines.qty, Customers.Name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustID">
            <Value>=Parameters!CustID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="department">
      <Fields>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="depid">
          <DataField>depid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PosLive</DataSourceName>
        <CommandText>SELECT     RTRIM(description) AS description, depid
FROM         dbo.r_departments
ORDER BY RTRIM(description)</CommandText>
      </Query>
    </DataSet>
  </DataSets>
